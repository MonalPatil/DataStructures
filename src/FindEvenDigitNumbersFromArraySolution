class FindEvenDigitNumbersFromArraySolution {
    public int findNumbers(int[] nums) {
        if(nums==null)
            return 0;
        int count =0;
        if(nums.length ==0)
            return 0;
        for(int i=0; i<nums.length;i++){
            int num = nums[i];
            
            String str = Integer.toString(num);
            if(str.length()%2 ==0){
                count++;
            }
        }
        return count;
        
    }
}
