class SearchInsertPositionInSortedArraySolution {
    public int searchInsert(int[] nums, int target) {
        int solution =0; 
        if(nums.length ==0){
            return 0;
        }
        else{
            for(int i=0;i<nums.length;i++){
                if(nums[i]== target || nums[i]>target)
                    return i;
                    
                    
            }
            
           return nums.length;
        }
            
        
      
        
    }
}
